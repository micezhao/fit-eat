<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.f.a.allan.dao.OrderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.f.a.allan.pojo.Order">
        <id column="id" property="id" />
        <result column="order_id" property="orderId" />
        <result column="customer_id" property="customerId" />
        <result column="total" property="total" />
        <result column="discount_price" property="discountPrice" />
        <result column="settlement_price" property="settlementPrice" />
        <result column="order_time" property="orderTime" />
        <result column="expire_time" property="expireTime" />
        <result column="close_time" property="closeTime" />
        <result column="finish_time" property="finishTime" />
        <result column="status" property="status" />
        <result column="cdt" property="cdt" />
        <result column="mdt" property="mdt" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, order_id, customer_id, total, discount_price, settlement_price, order_time, expire_time, close_time, finish_time, status, cdt, mdt
    </sql>

</mapper>
