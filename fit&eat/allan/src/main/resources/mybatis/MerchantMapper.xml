<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.f.a.allan.dao.MerchantMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.f.a.allan.pojo.Merchant">
        <id column="id" property="id" />
        <result column="merchant_no" property="merchantNo" />
        <result column="name" property="name" />
        <result column="register_date" property="registerDate" />
        <result column="type" property="type" />
        <result column="contacter" property="contacter" />
        <result column="contact_phone" property="contactPhone" />
        <result column="status" property="status" />
        <result column="cdt" property="cdt" />
        <result column="mdt" property="mdt" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, merchant_no, name, register_date, type, contacter, contact_phone, status, cdt, mdt
    </sql>

</mapper>
